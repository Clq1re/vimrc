{
  "list.insertMappings": {
		"<C-c>": "do:exit",
    "<C-j>":"do:next",
    "<C-k>":"do:previous"
	},
	"list.normalMappings": {
		"<C-c>": "do:exit"
	},
  "coc.preferences.colorSupport": true,
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "diagnostic.errorSign": "×",
  "diagnostic.warningSign": "×",
  "diagnostic.infoSign": "×",
  "eslint.autoFixOnSave": false,
  "prettier.eslintIntegration": true,
  "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript", "typescriptreact", "json", "javascriptreact", "vue"],
  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "vue"],
  "emmet.includeLanguages": {"vue-html": "html", "javascript": "javascriptreact", "tsx": "html"}
}
